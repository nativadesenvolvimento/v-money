{
  "name": "v-money",
  "version": "0.8.1",
  "description": "Vue 3 currency input/directive mask",
  "main": "dist/v-money.umd.min.js",
  "scripts": {
    "size": "size-limit",
    "upgrade:check": "ncu",
    "upgrade:apply": "ncu -a",
    "dev": "vue build ./src/docs/docs.vue --config ./src/docs/build.config.js --host 0.0.0.0",
    "build": "vue build --name v-money --target lib",
    "docs:build": "vue build ./src/docs/docs.vue --config ./src/docs/build.config.js --dist ./docs/ --prod",
    "docs:serve": "open http://localhost:5000/docs/ && serve ./",
    "compile": "npm run build -- --disable-compress",
    "test": "jest"
  },
  "keywords": [
    "vue",
    "input",
    "mask",
    "directive",
    "currency",
    "money"
  ],
  "files": [
    "dist/"
  ],
  "author": "Fork by Sylvester Charles <silverprince4134u@gmail.com> from (https://billitech.github.io/ Marcos Neves <marcos.neves@gmail.com> (https://vuejs-tips.github.io/)",
  "license": "MIT",
  "devDependencies": {
    "babel-jest": "^21.2.0",
    "jest": "^21.2.1",
    "npm-check-updates": "^2.13.0",
    "vuetify": "^0.16.7"
  },
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0"
  }
}
